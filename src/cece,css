@-webkit-keyframes scaleToggle {
    0% {
        -webkit-transform: scale(1);
        transform: scale(1)
    }

    25% {
        -webkit-transform: scale(1.05);
        transform: scale(1.05)
    }

    50% {
        -webkit-transform: scale(1);
        transform: scale(1)
    }

    75% {
        -webkit-transform: scale(1.05);
        transform: scale(1.05)
    }
}

@keyframes scaleToggle {
    0% {
        -webkit-transform: scale(1);
        transform: scale(1)
    }

    25% {
        -webkit-transform: scale(1.05);
        transform: scale(1.05)
    }

    50% {
        -webkit-transform: scale(1);
        transform: scale(1)
    }

